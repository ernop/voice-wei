{
  "name": "voice-music-control",
  "version": "0.0.16",
  "private": true,
  "description": "Voice-controlled music player and scale practice tool",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "typecheck:strict": "tsc --noEmit --strict",
    "lint": "ast-grep scan",
    "lint:quiet": "ast-grep scan --report-style short",
    "lint:ci": "ast-grep scan --format github"
  },
  "devDependencies": {
    "@ast-grep/cli": "^0.40.3",
    "@types/youtube": "^0.1.0",
    "tone": "^14.8.49",
    "typescript": "^5.7.2"
  }
}